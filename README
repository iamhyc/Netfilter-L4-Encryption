//Source Address of Header of L3#
skb->nh.iph->saddr

//Check the protocol from L3#
skb->nh.iph->protocol == IPPROTO_TCP
skb->nh.iph->protocol == IPPROTO_UDP
/*Get TCP Header*/
iph = ip_hdr(skb);
tcph = (void *) iph + iph->ihl * 4; ///TCP Header
dport = tcph->dest; //destination port of TCP, 
//ntohs:Network to HoSt;hston: HoSt to Network

